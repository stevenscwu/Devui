{
  "name": "ng-devui-admin",
  "version": "1.0.0",
  "license": "MIT",
  "description": "DevUI Admin based on Angular and DevUI components",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenscwu/Devui"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 8001",
    "build": "ng build",
    "build:website": "ng build --configuration production --base-href /admin/ --deploy-url /admin/ --output-path dist/admin",
    "test": "ng test",
    "lint": "eslint src --ext .ts,.html",
    "e2e": "ng e2e",
    "tslint": "tslint --fix -c ./tslint.json 'src/**/*{.ts}'",
    "prettier": "prettier --config ./.prettierrc --write \"src/app/pages/**/**/*.html\"",
    "stylelint": "stylelint \"src/app/pages/**/**/*.{scss,css}\" --config ./.stylelintrc.json --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.10",
    "@angular/cdk": "15.2.9",
    "@angular/common": "15.2.10",
    "@angular/compiler": "15.2.10",
    "@angular/compiler-cli": "15.2.10",
    "@angular/core": "15.2.10",
    "@angular/forms": "15.2.10",
    "@angular/platform-browser": "15.2.10",
    "@angular/platform-browser-dynamic": "15.2.10",
    "@angular/router": "15.2.10",
    "@devui-design/icons": "^1.3.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "color": "^3.1.3",
    "echarts": "5.2.1",
    "gridstack": "^6.0.0",
    "ng-devui": "^15.0.0",
    "rxjs": "7.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.10",
    "@angular/cli": "15.2.10",
    "@angular/compiler-cli": "15.2.10",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.47.0",
    "azure-functions-core-tools": "^4.1.0",
    "codelyzer": "^6.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "shelljs": "^0.8.4",
    "stylelint": "^16.22.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-scss": "^6.12.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5",
    "typescript-eslint": "8.34.1"
  }
